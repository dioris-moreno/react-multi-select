{
    "name": "@khanacademy/react-multi-select",
    "version": "0.3.2",
    "description": "React Multi Select Component",
    "repository": {
        "type": "git",
        "url": "https://github.com/khan/react-multi-select"
    },
    "license": "MIT",
    "scripts": {
        "prepare": ". ./.scripts/prepublish.sh",
        "lint": "eslint src",
        "lintfix": "eslint src --fix",
        "testonly": "mocha --require .scripts/mocha_runner src/**/tests/**/*.js",
        "test": "npm run lint && npm run testonly",
        "test-watch": "npm run testonly -- --watch --watch-extensions js",
        "storybook": "start-storybook -p 9010",
        "publish-storybook": "bash .scripts/publish_storybook.sh",
        "flow": "flow; test $? -eq 0 -o $? -eq 2"
    },
    "devDependencies": {
        "@storybook/addons": "^5.1.11",
        "@storybook/react": "^5.1.11",
        "babel-cli": "^6.14.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.0.2",
        "babel-loader": "^8.0.6",
        "babel-plugin-flow-react-proptypes": "^25.1.0",
        "babel-plugin-transform-flow-strip-types": "^6.22.0",
        "babel-plugin-transform-object-rest-spread": "^6.23.0",
        "babel-plugin-transform-runtime": "^6.15.0",
        "babel-polyfill": "^6.13.0",
        "babel-preset-react-app": "^9.0.1",
        "babel-runtime": "^6.26.0",
        "chai": "^4.2.0",
        "enzyme": "^3.10.0",
        "escope": "^3.6.0",
        "eslint": "^6.1.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-plugin-flowtype": "^4.2.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.14.3",
        "flow-bin": "^0.105.2",
        "git-url-parse": "^11.1.2",
        "jsdom": "^15.1.1",
        "react": "^16.9.0",
        "react-addons-test-utils": "^15.6.2",
        "react-dom": "^16.9.0",
        "sinon": "^7.4.1",
        "uglify-js": "^3.6.0",
        "webpack": "^4.39.2"
    },
    "peerDependencies": {
        "react": "^0.14.7 || ^16.0.0",
        "typescript": "*"
    },
    "dependencies": {
        "fuzzy-match-utils": "^1.2.0"
    },
    "main": "dist/index.js",
    "engines": {
        "npm": "^3.0.0"
    }
}
